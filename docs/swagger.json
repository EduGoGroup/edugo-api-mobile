{
    "swagger": "2.0",
    "info": {
        "description": "API para operaciones frecuentes de docentes y estudiantes en EduGo",
        "title": "EduGo API Mobile",
        "termsOfService": "http://edugo.com/terms/",
        "contact": {
            "name": "Equipo EduGo",
            "email": "soporte@edugo.com"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/v1",
    "paths": {
        "/auth/login": {
            "post": {
                "description": "Authenticate user and return JWT token",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "User login",
                "parameters": [
                    {
                        "description": "Login credentials",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.LoginResponse"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "$ref": "#/definitions/handler.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "Verifica que la API está funcionando",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Health"
                ],
                "summary": "Health check",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/materials": {
            "get": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "List materials",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/dto.MaterialResponse"
                            }
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Creates a new educational material",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "Create a new material",
                "parameters": [
                    {
                        "description": "Material data",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.CreateMaterialRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/dto.MaterialResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/handler.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/materials/{id}": {
            "get": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "Get material by ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Material ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.MaterialResponse"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/handler.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/materials/{id}/assessment": {
            "get": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "Get material assessment/quiz",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Material ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/repository.MaterialAssessment"
                        }
                    }
                }
            }
        },
        "/materials/{id}/assessment/attempts": {
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "Record assessment attempt",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Material ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Answers",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/repository.AssessmentAttempt"
                        }
                    }
                }
            }
        },
        "/materials/{id}/progress": {
            "patch": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "Update reading progress",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Material ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Progress data",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "integer"
                            }
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content"
                    }
                }
            }
        },
        "/materials/{id}/stats": {
            "get": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "Get material statistics",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Material ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/service.MaterialStats"
                        }
                    }
                }
            }
        },
        "/materials/{id}/summary": {
            "get": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "Get material summary",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Material ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/repository.MaterialSummary"
                        }
                    }
                }
            }
        },
        "/materials/{id}/upload-complete": {
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Notify that PDF upload to S3 is complete",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "materials"
                ],
                "summary": "Notify upload complete",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Material ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "S3 info",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.UploadCompleteRequest"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/handler.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "Answer": {
            "type": "object",
            "required": [
                "answer",
                "question_id"
            ],
            "properties": {
                "answer": {
                    "type": "string",
                    "example": "option_a"
                },
                "question_id": {
                    "type": "string",
                    "example": "q-uuid-1"
                }
            }
        },
        "AssessmentResponse": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string",
                    "example": "Evaluación de conceptos básicos"
                },
                "passing_score": {
                    "type": "integer",
                    "example": 70
                },
                "questions": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Question"
                    }
                },
                "time_limit_minutes": {
                    "type": "integer",
                    "example": 15
                },
                "title": {
                    "type": "string",
                    "example": "Cuestionario: Introducción a Pascal"
                },
                "total_points": {
                    "type": "integer",
                    "example": 100
                },
                "total_questions": {
                    "type": "integer",
                    "example": 5
                }
            }
        },
        "AttemptResultResponse": {
            "type": "object",
            "properties": {
                "detailed_feedback": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/QuestionFeedback"
                    }
                },
                "passed": {
                    "type": "boolean",
                    "example": true
                },
                "score": {
                    "type": "number",
                    "example": 85.5
                },
                "total_points": {
                    "type": "number",
                    "example": 100
                }
            }
        },
        "CreateMaterialRequest": {
            "type": "object",
            "required": [
                "description",
                "file_name",
                "file_size",
                "subject_id",
                "title",
                "unit_id"
            ],
            "properties": {
                "description": {
                    "type": "string",
                    "example": "Material base sobre programación en Pascal"
                },
                "file_name": {
                    "type": "string",
                    "example": "introduccion-pascal.pdf"
                },
                "file_size": {
                    "type": "integer",
                    "example": 2048576
                },
                "subject_id": {
                    "type": "string",
                    "example": "subject-uuid-123"
                },
                "title": {
                    "type": "string",
                    "example": "Introducción a Pascal"
                },
                "unit_id": {
                    "type": "string",
                    "example": "unit-uuid-456"
                }
            }
        },
        "CreateMaterialResponse": {
            "type": "object",
            "properties": {
                "material_id": {
                    "type": "string",
                    "example": "uuid-123"
                },
                "max_file_size_bytes": {
                    "type": "integer",
                    "example": 104857600
                },
                "status": {
                    "type": "string",
                    "example": "created"
                },
                "upload_url": {
                    "type": "string",
                    "example": "https://s3.amazonaws.com/edugo-materials-prod/upload-url-mock"
                },
                "upload_url_expires_at": {
                    "type": "string",
                    "example": "2025-01-29T12:15:00Z"
                }
            }
        },
        "ErrorResponse": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string",
                    "example": "Invalid request"
                },
                "message": {
                    "type": "string",
                    "example": "Field 'title' is required"
                }
            }
        },
        "GlossaryTerm": {
            "type": "object",
            "properties": {
                "definition": {
                    "type": "string",
                    "example": "Programa que traduce código fuente a código máquina"
                },
                "order": {
                    "type": "integer",
                    "example": 1
                },
                "term": {
                    "type": "string",
                    "example": "Compilador"
                }
            }
        },
        "LoginRequest": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "example": "maria@example.com"
                },
                "password": {
                    "type": "string",
                    "example": "password123"
                }
            }
        },
        "LoginResponse": {
            "type": "object",
            "properties": {
                "expires_at": {
                    "type": "string",
                    "example": "2025-01-29T12:00:00Z"
                },
                "refresh_token": {
                    "type": "string",
                    "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                },
                "token": {
                    "type": "string",
                    "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                },
                "user": {
                    "$ref": "#/definitions/UserInfo"
                }
            }
        },
        "MaterialDetail": {
            "type": "object",
            "properties": {
                "author_name": {
                    "type": "string",
                    "example": "Prof. García"
                },
                "created_at": {
                    "type": "string",
                    "example": "2025-01-15T12:00:00Z"
                },
                "description": {
                    "type": "string",
                    "example": "Material base sobre Pascal"
                },
                "file_size": {
                    "type": "integer",
                    "example": 2048576
                },
                "id": {
                    "type": "string",
                    "example": "uuid-1"
                },
                "my_progress": {
                    "type": "number",
                    "example": 0
                },
                "subject_name": {
                    "type": "string",
                    "example": "Programación"
                },
                "title": {
                    "type": "string",
                    "example": "Introducción a Pascal"
                }
            }
        },
        "MaterialDetailResponse": {
            "type": "object",
            "properties": {
                "has_quiz": {
                    "type": "boolean",
                    "example": true
                },
                "has_summary": {
                    "type": "boolean",
                    "example": true
                },
                "material": {
                    "$ref": "#/definitions/MaterialDetail"
                },
                "pdf_url": {
                    "type": "string",
                    "example": "https://s3.../presigned-url"
                },
                "pdf_url_expires_at": {
                    "type": "string",
                    "example": "2025-01-29T11:15:00Z"
                }
            }
        },
        "MaterialListResponse": {
            "type": "object",
            "properties": {
                "materials": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/MaterialSummary"
                    }
                },
                "page": {
                    "type": "integer"
                },
                "total": {
                    "type": "integer"
                }
            }
        },
        "MaterialStatsResponse": {
            "type": "object",
            "properties": {
                "average_progress": {
                    "type": "number",
                    "example": 65.5
                },
                "average_score": {
                    "type": "number",
                    "example": 82.3
                },
                "total_completed": {
                    "type": "integer",
                    "example": 45
                },
                "total_views": {
                    "type": "integer",
                    "example": 150
                }
            }
        },
        "MaterialSummary": {
            "type": "object",
            "properties": {
                "has_quiz": {
                    "type": "boolean",
                    "example": true
                },
                "has_summary": {
                    "type": "boolean",
                    "example": true
                },
                "id": {
                    "type": "string",
                    "example": "uuid-1"
                },
                "progress": {
                    "type": "number",
                    "example": 0
                },
                "published_at": {
                    "type": "string",
                    "example": "2025-01-15T12:00:00Z"
                },
                "status": {
                    "type": "string",
                    "enum": [
                        "new",
                        "in_progress",
                        "completed"
                    ],
                    "example": "new"
                },
                "subject_name": {
                    "type": "string",
                    "example": "Programación"
                },
                "title": {
                    "type": "string",
                    "example": "Introducción a Pascal"
                },
                "unit_name": {
                    "type": "string",
                    "example": "5.º A - Programación"
                }
            }
        },
        "MaterialSummaryResponse": {
            "type": "object",
            "properties": {
                "glossary": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/GlossaryTerm"
                    }
                },
                "processing_metadata": {
                    "$ref": "#/definitions/ProcessingMetadata"
                },
                "reflection_questions": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "¿Cómo aplicarías este concepto?",
                        "¿Qué diferencias encuentras?"
                    ]
                },
                "sections": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/SummarySection"
                    }
                }
            }
        },
        "ProcessingMetadata": {
            "type": "object",
            "properties": {
                "language": {
                    "type": "string",
                    "example": "es"
                },
                "model": {
                    "type": "string",
                    "example": "gpt-4"
                },
                "nlp_provider": {
                    "type": "string",
                    "example": "openai"
                },
                "processing_time_seconds": {
                    "type": "integer",
                    "example": 45
                },
                "prompt_version": {
                    "type": "string",
                    "example": "v1.2"
                },
                "tokens_used": {
                    "type": "integer",
                    "example": 3500
                }
            }
        },
        "Question": {
            "type": "object",
            "properties": {
                "difficulty": {
                    "type": "string",
                    "enum": [
                        "basic",
                        "medium",
                        "advanced"
                    ],
                    "example": "basic"
                },
                "id": {
                    "type": "string",
                    "example": "q1"
                },
                "options": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/QuestionOption"
                    }
                },
                "order": {
                    "type": "integer",
                    "example": 1
                },
                "points": {
                    "type": "integer",
                    "example": 20
                },
                "text": {
                    "type": "string",
                    "example": "¿Qué es un compilador?"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "multiple_choice",
                        "true_false",
                        "short_answer"
                    ],
                    "example": "multiple_choice"
                }
            }
        },
        "QuestionFeedback": {
            "type": "object",
            "properties": {
                "feedback_message": {
                    "type": "string",
                    "example": "¡Correcto! Un compilador traduce código fuente a código máquina."
                },
                "is_correct": {
                    "type": "boolean",
                    "example": true
                },
                "question_id": {
                    "type": "string",
                    "example": "q1"
                },
                "your_answer": {
                    "type": "string",
                    "example": "a"
                }
            }
        },
        "QuestionOption": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "a"
                },
                "text": {
                    "type": "string",
                    "example": "Un programa que traduce código"
                }
            }
        },
        "RecordAttemptRequest": {
            "type": "object",
            "required": [
                "answers"
            ],
            "properties": {
                "answers": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Answer"
                    }
                }
            }
        },
        "SuccessResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string",
                    "example": "Operation completed successfully"
                },
                "status": {
                    "type": "string",
                    "example": "success"
                }
            }
        },
        "SummarySection": {
            "type": "object",
            "properties": {
                "content": {
                    "type": "string",
                    "example": "Pascal fue desarrollado..."
                },
                "difficulty": {
                    "type": "string",
                    "enum": [
                        "basic",
                        "medium",
                        "advanced"
                    ],
                    "example": "basic"
                },
                "estimated_time_minutes": {
                    "type": "integer",
                    "example": 5
                },
                "order": {
                    "type": "integer",
                    "example": 1
                },
                "title": {
                    "type": "string",
                    "example": "Contexto Histórico"
                }
            }
        },
        "UpdateProgressRequest": {
            "type": "object",
            "required": [
                "progress"
            ],
            "properties": {
                "progress": {
                    "type": "number",
                    "maximum": 100,
                    "minimum": 0,
                    "example": 75.5
                }
            }
        },
        "UploadCompleteRequest": {
            "type": "object",
            "required": [
                "checksum",
                "file_size",
                "version_key"
            ],
            "properties": {
                "checksum": {
                    "type": "string",
                    "example": "sha256:abc123..."
                },
                "file_size": {
                    "type": "integer",
                    "example": 2048576
                },
                "version_key": {
                    "type": "string",
                    "example": "materials/uuid-1/v1.pdf"
                }
            }
        },
        "UploadCompleteResponse": {
            "type": "object",
            "properties": {
                "material_id": {
                    "type": "string",
                    "example": "uuid-123"
                },
                "message": {
                    "type": "string",
                    "example": "Material queued for processing"
                },
                "status": {
                    "type": "string",
                    "example": "processing"
                }
            }
        },
        "UserInfo": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string",
                    "example": "maria@example.com"
                },
                "id": {
                    "type": "string",
                    "example": "user-uuid-123"
                },
                "name": {
                    "type": "string",
                    "example": "María González"
                },
                "role": {
                    "type": "string",
                    "example": "teacher"
                }
            }
        },
        "dto.CreateMaterialRequest": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string"
                },
                "subject_id": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "dto.LoginRequest": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            }
        },
        "dto.LoginResponse": {
            "type": "object",
            "properties": {
                "expires_at": {
                    "type": "string"
                },
